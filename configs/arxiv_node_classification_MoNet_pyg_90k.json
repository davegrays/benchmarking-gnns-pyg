{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "MoNet_pyg",
    
    "out_dir": "out/ogb_node_classification/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 70,
        "out_dim": 70,
        "residual": true,
        "edge_feat": false,
        "readout": "mean",
        "kernel": 3,
        "pseudo_dim_MoNet": 2,
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true
    }
}